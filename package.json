{
    "name": "vscode-hyperscript-org",
    "displayName": "_hyperscript",
    "publisher": "tomlindet",
    "description": "language support for _hyperscript <https://hyperscript.org>",
    "icon": "logo.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/tomlindet/vscode-hyperscript.git"
    },
    "version": "0.1.6",
    "engines": {
        "vscode": "^1.59.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "_hyperscript",
                "aliases": [
                    "_hyperscript",
                    "_hyperscript"
                ],
                "extensions": [
                    "._hs"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "_hyperscript",
                "scopeName": "source._hyperscript",
                "path": "./hyperscript-tmlanguage/_hyperscript.tmLanguage.json",
                "embeddedLanguages": {
                    "meta.embedded.line.js": "javascript"
                }
            },
            {
                "injectTo": [
                    "text.html",
                    "meta.embedded.block.html"
                ],
                "embeddedLanguages": {
                    "meta.embedded.line._hyperscript": "_hyperscript"
                },
                "scopeName": "source.hyperscript-html-underscore-attr.injection",
                "path": "./syntaxes/html-attr-injection.tmLanguage.json"
            },
            {
                "injectTo": [
                    "text.html",
                    "meta.embedded.block.html"
                ],
                "embeddedLanguages": {
                    "meta.embedded.block._hyperscript": "_hyperscript"
                },
                "scopeName": "source.hyperscript-html-script-element.injection",
                "path": "./syntaxes/html-script-element-injection.tmLanguage.json"
            },
            {
                "injectTo": [
                    "text.html.markdown"
                ],
                "embeddedLanguages": {
                    "meta.embedded.block._hyperscript": "_hyperscript"
                },
                "scopeName": "markdown._hyperscript.codeblock",
                "path": "./syntaxes/markdown-injection.tmLanguage.json"
            },
            {
                "injectTo": [
                    "source.tsx",
                    "source.jsx"
                ],
                "embeddedLanguages": {
                    "meta.embedded.block._hyperscript": "_hyperscript"
                },
                "scopeName": "source.hyperscript-script-tsx.injection",
                "path": "./syntaxes/tsx-script-injection.tmLanguage.json"
            },
            {
                "injectTo": [
                    "source.tsx",
                    "source.jsx"
                ],
                "embeddedLanguages": {
                    "meta.embedded.line._hyperscript": "_hyperscript"
                },
                "scopeName": "source.hyperscript-attr-tsx.injection",
                "path": "./syntaxes/tsx-attr-injection.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "_hyperscript",
                "path": "./snippets/snippets.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^4.1.0",
        "plist2": "^1.1.2"
    },
    "license": "MIT",
    "dependencies": {
        "hono": "^4.11.8"
    }
}