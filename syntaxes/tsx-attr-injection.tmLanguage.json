{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "scopeName": "source.hyperscript-attr-tsx.injection",
  "injectionSelector": "L:meta.tag.attributes.tsx",
  "patterns": [
    {
      "begin": "\\b_\\s*=\\s*([\"'])",
      "beginCaptures": {
        "1": { "name": "punctuation.definition.string.begin.tsx" }
      },
      "end": "\\1",
      "name": "meta.embedded.line._hyperscript",
      "patterns": [
        { "include": "source._hyperscript" }
      ]
    }
  ]
}